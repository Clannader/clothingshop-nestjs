{
  "name": "@andybeat/clothingshop-nestjs",
  "version": "2.10.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "description": "Typescript Backend Server",
  "author": {
    "name": "AndyBeat",
    "email": "137811032@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndyBeat/clothingshop-nestjs.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production && npm run build:dev && npm run build:zip",
    "build:dev": "nest build -c nest-cli-build.json",
    "build:prod": "npm run test && npm run test:e2e && npm run build",
    "build:prod:lint": "npm run lint && npm run build:prod",
    "build:zip": "node scripts/build.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"scripts/**/*.js\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node build/main",
    "publish:npm": "npm publish --access public",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@andybeat/schedule": "^2.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@node-saml/passport-saml": "^5.1.0",
    "agentkeepalive": "^4.5.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.3",
    "cache-manager": "^6.4.3",
    "cacheable": "^1.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect-mongo": "^5.1.0",
    "cookie": "^0.7.2",
    "cookie-parser": "^1.4.7",
    "cookie-signature": "^1.2.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "dotenv-expand": "^12.0.2",
    "ejs": "^3.1.10",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "keyv": "^5.5.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "mongoose": "~8.14.3",
    "node-xss": "0.0.1",
    "on-finished": "^2.4.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "speakeasy": "^2.0.0",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^5.0.1",
    "tunnel": "^0.0.6",
    "uuid": "^11.0.3",
    "validator": "^13.15.15",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cookie-signature": "^1.1.2",
    "@types/crypto-js": "^4.2.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.17.0",
    "@types/on-finished": "^2.3.5",
    "@types/passport-saml": "^1.1.7",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/tunnel": "^0.0.7",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.38.0",
    "ajv": "^8.17.1",
    "archiver": "^7.0.1",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.build.json"
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
